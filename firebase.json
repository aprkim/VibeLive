{
  "hosting": [
    {
      "target": "home",
      "public": "out-home",
      "cleanUrls": true,
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "headers": [
        {
          "source": "**/*.@(js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2)",
          "headers": [
            { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
          ]
        }
      ],
      "redirects": [
        {
          "source": "/docs",
          "destination": "https://docs.vibelive.site",
          "type": 301
        },
        {
          "source": "/get-started",
          "destination": "/get-started.md",
          "type": 301
        }
      ],
      "rewrites": []
    },
    {
      "target": "built",
      "public": "out-built",
      "cleanUrls": true,
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
